tmux_running=$(pgrep tmux)
frontend_window_name=wuardi-front
frontend_path=/home/alex/Tinkin/WUARDI/wuardi_front
backend_window_name=wuardi-back
backend_path=/home/alex/Tinkin/WUARDI/wuardi_back

if [[ -z $TMUX ]] && [[ -z $tmux_running ]]; then
    tmux new-session -ds $frontend_window_name -c $frontend_path
fi

if [[ -z $TMUX ]] && [[ -z $tmux_running ]]; then
    tmux new-session -ds $backend_window_name -c $backend_path
fi
